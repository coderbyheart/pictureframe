{
  "name": "pictureframe",
  "version": "0.0.0-development",
  "description": "A simple picture frame website",
  "main": "index.js",
  "config": {
    "appName": "Picture Frame",
    "baseHref": "/"
  },
  "scripts": {
    "test": "mocha --require babel-polyfill --compilers js:babel-register --reporter=dot --timeout=1350 test/\\*.js",
    "dev-server": "static build",
    "lint": "standard",
    "precommit": "npm run lint && npm test",
    "commitmsg": "validate-commit-msg"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coderbyheart/pictureframe.git"
  },
  "author": "Markus Tacker <m@coderbyheart.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/coderbyheart/pictureframe/issues"
  },
  "homepage": "https://github.com/coderbyheart/pictureframe#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "chai": "^4.0.2",
    "husky": "^0.13.4",
    "mocha": "^3.4.2",
    "node-sass": "^4.5.3",
    "node-static": "^0.7.9",
    "request-promise": "^4.2.1",
    "rheactor-build-views": "4.x",
    "standard": "^10.0.2",
    "uglifycss": "0.0.27",
    "uglifyjs": "^2.4.11",
    "validate-commit-msg": "^2.12.2"
  },
  "dependencies": {
    "jquery": "^3.2.1",
    "velocity-animate": "^1.5.0"
  }
}
